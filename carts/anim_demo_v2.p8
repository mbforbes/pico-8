pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
function _init()
	--custom vars defined
	-- heart = 1
	heart = {
	  -- permanent state / settings
	  first=1,
	  last=4,
	  speed=0.2,
	  -- current state / transient
	  cur=1,
	}
end

	--custom functions defined
function _anim(_x, _y, _sprite)
		spr(_sprite.cur, _x, _y)
		
		_sprite.cur += _sprite.speed
		if _sprite.cur >= _sprite.last + 1 then
			_sprite.cur = _sprite.first
		end
		
		print(_sprite.cur)	
end
	
	
function _update()
--	do stuff
end

function _draw()
	cls() --clears screen
	_anim(63, 63, heart)
end
